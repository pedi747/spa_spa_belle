{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/favicon.ico", "destination": "/favicon.ico" },
    { "source": "/:match*", "destination": "/:match*" }
  ],
  "staticFiles": {
    "include": ["favicon.ico", "favicon.png", "robots.txt", "sitemap.xml"]
  }
}